# ANDB Environment Variables Example
# Copy this file to .env and fill in your database credentials

# Development Database
DEV_DB_HOST=localhost
DEV_DB_PORT=3306
DEV_DB_NAME=dev_database
DEV_DB_USER=dev_user
DEV_DB_PASS=dev_password

# Staging Database
STAGE_DB_HOST=stage-server.com
STAGE_DB_PORT=3306
STAGE_DB_NAME=stage_database
STAGE_DB_USER=stage_user
STAGE_DB_PASS=stage_password

# UAT Database
UAT_DB_HOST=uat-server.com
UAT_DB_PORT=3306
UAT_DB_NAME=uat_database
UAT_DB_USER=uat_user
UAT_DB_PASS=uat_password

# Production Database
PROD_DB_HOST=prod-server.com
PROD_DB_PORT=3306
PROD_DB_NAME=prod_database
PROD_DB_USER=prod_user
PROD_DB_PASS=prod_password

# Optional: Logging
MODE=PROD
LOG_LEVEL=info
